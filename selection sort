#include <iostream>

using namespace std;
int selectionsort(int a[],int n){
for(int i=0;i<n;i++)
{
    int t=i;
    for(int j=i+1;j<n;j++
    {
        if(a[j]<a[t])
        {
            t=j;
        }

    }
    swap(a[t],a[i]);
}
}

int main()
{
int n;
cin>>n;
int a[n];
for(int i=0;i<n;i++)
{
    cin>>a[i];
}
selectionsort(a,n);
for(int i=0;i<n;i++)
{
    cout<<a[i]<<" ";
}
}
